    <RepositoryUrl>https://github.com/Penca53/variant-csharp</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/Penca53/variant-csharp/blob/main/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright Â© 2021 Penca53</Copyright>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Penca53.JsonConverters" Version="1.0.1" />
    <PackageReference Include="System.Text.Json" Version="5.0.2" />
  </ItemGroup>
</Project>
